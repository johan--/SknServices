<% provide(:title, accessed_page_name) %>

<section class="container">
    <h1><%= accessed_page_name %></h1>

    <%= will_paginate class: "pagination col-xs-3 col-md-offset-9 pull-right" %>
        <section class="container">
            <%= render @users %>
        </section>
    <%= will_paginate class: "pagination col-xs-3 col-md-offset-9 pull-right" %>

    <% if current_user_has_create?('users/new') %>
        <%= link_to 'New User group role', new_user_path %>
    <% end %>
</section>