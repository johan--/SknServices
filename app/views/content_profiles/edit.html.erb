<% provide(:title, accessed_page_name) %>

<header class="page-header">
    <h1 class="text-center"><%= accessed_page_name %></h1>
</header>

<%= render 'form' %>

<hr/>

<section>
  <div class="col-md-6">
    <h4 class="text-center"><em>Existing ContentProfile Entries</em></h4>
    <% @page_controls.content_profile_entries.each do |cpe| %>
        <%= render 'content_profile_entry', entry: cpe  %>
    <% end %>
  </div>
  <div class="col-md-6">
    <h4 class="text-center"><em>Edit by selecting entry on left.</em></h4>
  </div>
</section>
