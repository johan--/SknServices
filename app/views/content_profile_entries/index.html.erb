<p id="notice"><%= notice %></p>

<h1>Listing Content Profile Entries</h1>

<table>
  <thead>
    <tr>
      <th>Topic value</th>
      <th>Content value</th>
      <th>Content type</th>
      <th>Topic type</th>
      <th>Content profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @content_profile_entries.each do |content_profile_entry| %>
      <tr>
        <td><%= content_profile_entry.topic_value %></td>
        <td><%= content_profile_entry.content_value %></td>
        <td><%= content_profile_entry.content_type %></td>
        <td><%= content_profile_entry.topic_type %></td>
        <td><%= content_profile_entry.content_profile %></td>
        <td><%= link_to 'Show', content_profile_entry %></td>
        <td><%= link_to 'Edit', edit_content_profile_entry_path(content_profile_entry) %></td>
        <td><%= link_to 'Destroy', content_profile_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Content profile entry', new_content_profile_entry_path %>
